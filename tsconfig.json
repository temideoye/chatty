{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"allowJs": true,
		"jsx": "react",
		"noEmit": true,
		"strict": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
